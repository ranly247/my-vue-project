(function(e,d){var b=5,c=["click","touchstart","touchmove","touchend"],a={click:function(f){if(f.target===f.currentTarget){f.preventDefault()}},touchstart:function(f){this.jQueryTap.touched=true;this.jQueryTap.startX=f.touches[0].pageX;this.jQueryTap.startY=f.touches[0].pageY},touchmove:function(f){if(!this.jQueryTap.touched){return}if(Math.abs(f.touches[0].pageX-this.jQueryTap.startX)>b||Math.abs(f.touches[0].pageY-this.jQueryTap.startY)>b){this.jQueryTap.touched=false}},touchend:function(f){if(!this.jQueryTap.touched){return}this.jQueryTap.touched=false;e.event.dispatch.call(this,e.Event("tap",{originalEvent:f,target:f.target,pageX:f.changedTouches[0].pageX,pageY:f.changedTouches[0].pageY}))}};e.event.special.tap="ontouchend" in d?{setup:function(){var f=this;if(!this.jQueryTap){Object.defineProperty(this,"jQueryTap",{value:{}})}e.each(c,function(g,h){f.addEventListener(h,a[h],false)})},teardown:function(){var f=this;e.each(c,function(g,h){f.removeEventListener(h,a[h],false)})}}:{bindType:"click",delegateType:"click"};e.fn.tap=function(g,f){return arguments.length>0?this.on("tap",null,g,f):this.trigger("tap")}})(jQuery,this);$(function(){$('a[href^="#"]').on("tap",function(){if(window.matchMedia("(min-width: 768px)").matches){var d=0}else{var d=45}var b=$(this).attr("href");var c=$(b=="#"||b==""?"html":b);var a=c.offset().top-d;$("body,html").animate({scrollTop:a},500);return false})});$(function(){var a=$("#pagetop");$(window).scroll(function(){if($(this).scrollTop()>50){a.fadeIn()}else{a.fadeOut()}});a.click(function(){$("body,html").animate({scrollTop:0},500);return false})});